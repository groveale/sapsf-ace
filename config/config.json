{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "time-off-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/timeOff/TimeOffAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/timeOff/TimeOffAdaptiveCardExtension.manifest.json"
        }
      ]
    },
    "time-off-azure-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/timeOffAzure/TimeOffAzureAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/timeOffAzure/TimeOffAzureAdaptiveCardExtension.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "TimeOffAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/timeOff/loc/{locale}.js",
    "TimeOffAzureAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/timeOffAzure/loc/{locale}.js"
  }
}
